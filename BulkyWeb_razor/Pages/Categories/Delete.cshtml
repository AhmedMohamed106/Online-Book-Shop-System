@page
@model BulkyWeb_razor.Pages.Categories.DeleteModel


<h2>Confirm Delete</h2>

<p>Are you sure you want to delete the category: @Model.category.Name?</p>

<form method="post">
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="/Cateories/Index" class="btn btn-secondary">Cancel</a>
</form>
